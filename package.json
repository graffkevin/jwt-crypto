{
  "name": "jwt-crypto",
  "version": "1.0.6",
  "description": "JWT Crypto",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bundle.esm.js",
      "require": "./dist/bundle.cjs.js"
    },
    "./dist/*": {
      "require": "./dist/*.cjs.js",
      "import": "./dist/*.esm.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "test": "jest --config jest.config.ts"
  },
  "keywords": [
    "jwt",
    "encode",
    "decode",
    "sign",
    "token",
    "verify",
    "npm",
    "javascript"
  ],
  "type": "module",
  "dependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.6",
    "crypto-js": "^4.2.0",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "vite": "^5.3.1",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
